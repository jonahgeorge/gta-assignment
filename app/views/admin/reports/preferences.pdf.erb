<% @users.each do |user| %>
  <h4><%= user.name %></h4>
  <table border="1" width="100%">
    <% user.preferences.each do |p| %>
      <tr>
        <td><%= p.course.department.subject_code %><%= p.course.course_number %></td>
        <td><%= p.value %></td>
      </tr>
    <% end %>
  </table>
  <br>
<% end %>
